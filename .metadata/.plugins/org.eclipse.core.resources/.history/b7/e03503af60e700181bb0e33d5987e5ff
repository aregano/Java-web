package com.alvaro.models;

public class Cancion {

	 private int id;
	 private String name;
	 private String author;
	 private String mp3;
	 
	public Usuario(int id, String name, String author, String mp3) {
		super();
		this.id = id;
		this.name = name;
		this.author = author;
		this.mp3 = mp3;
	}
	
}
